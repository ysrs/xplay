TEMPLATE = app
TARGET = XPlay
QT += core opengl gui multimedia widgets openglextensions
CONFIG += debug console
DEFINES += QT_DLL QT_OPENGL_LIB QT_OPENGLEXTENSIONS_LIB QT_WIDGETS_LIB QT_MULTIMEDIA_LIB

win32{
    INCLUDEPATH += ./GeneratedFiles \
        . \
        ./GeneratedFiles/$(ConfigurationName) \
        ./../../include
    LIBS += -L"./../../lib/win32" \
        -lopengl32 \
        -lglu32
    DESTDIR = ../../bin/win32
}

linux{
    INCLUDEPATH += /usr/local/ffmpeg/include
    LIBS += -L/usr/local/ffmpeg/lib -lavformat -lavcodec -lavutil -lswresample
    DESTDIR = ../../bin/linux64
}

DEPENDPATH += .
OBJECTS_DIR += debug
include(XPlay.pri)
message("qmake running...")

